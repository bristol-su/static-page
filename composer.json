{
    "name": "bristol-su/static-page",
    "description": "A static page module for the Bristol SU Portal Project, to display text or HTML",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Toby Twigger",
            "email": "tobytwigger1@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "bristol-su/support": "dev-portal-v4 as 4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.19",
        "laravel/legacy-factories": "^1.1",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "BristolSU\\Module\\StaticPage\\": "app/"
        },
        "classmap": [
            "database/factories/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BristolSU\\Module\\Tests\\StaticPage\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BristolSU\\Module\\StaticPage\\ModuleServiceProvider"
            ]
        }
    }
}
